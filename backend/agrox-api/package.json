	"scripts": {
		"deploy": "gcloud app deploy --quiet",
		"prebuild": "del-cli dist",
		"gcp-build": "echo 'Running gcp-build!!' && prisma generate && npm run build",
		"build": "del-cli dist && tsc",
		"start": "npm run start:prod",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"compiles": "tsc --noEmit",
		"prescription": "npm run build && node dist/dev-scripts/generate-prescription.js",
		"quotation": "npm run build && node dist/dev-scripts/generate-quotation.js",
		"parse-prescription": "npm run build && node dist/dev-scripts/parse-prescription.js",
		"migrate-db": "npx prisma migrate save --experimental"
	},
